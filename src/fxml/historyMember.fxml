                    <image>
                        <Image url="@../fxml/logo.png" />
                    </image>
                </ImageView>
                <Separator orientation="VERTICAL" prefHeight="50.0" prefWidth="75.0" visible="false" />
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="99.0" prefWidth="99.0" text="BOOKS" textFill="#63ac29">
                    <font>
                        <Font name="SansSerif Bold" size="15.0" />
                    </font>
                </Label>
                <Separator orientation="VERTICAL" prefHeight="71.0" prefWidth="30.0" visible="false" />
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="79.0" prefWidth="122.0" text="HISTORY" textFill="#63ac29" underline="true">
