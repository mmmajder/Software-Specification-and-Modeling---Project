            <TableColumn prefWidth="110.0" text="Return date" />
            <TableColumn minWidth="0.0" prefWidth="189.0" text="Status" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
